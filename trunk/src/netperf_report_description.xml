<?xml version="1.0" standalone="no" ?>
<!DOCTYPE commands SYSTEM "http://www.netperf.org/netperf_docs.dtd/1.0" >

<!-- ***WARNING*** This is a prototype version of a report description file.
                   It is intended for generic report generator development.
  -->

<report_description
  xmlns           = "http://www.netperf.org/ns/netperf">
  name            = "Default netperf4 Report Description"
  version         = "1.0.0"
  field_separator = " "
  part_separator  = "\n\n" >


<!--  default data sets always generated by netperf4
  <data_set
    name          = "CPU Util" />
  <data_set
    name          = "Run Time" />
  -->

<!--  data sets into which test results are accumulated -->
  <data_set  name = "Network Total" >
    <cntr_attributes
      cntr_agg_flag = "TRUE"
      cntr_unit     = "bytes"
      cntr_type     = "in"
      cntr_class    = "lan" />
    <cntr_attributes
      cntr_agg_flag = "TRUE"
      cntr_unit     = "bytes"
      cntr_type     = "out"
      cntr_class    = "lan" />
  </data_set>
  <data_set  name = "Network Recv" >
    <cntr_attributes
      cntr_agg_flag = "TRUE"
      cntr_unit     = "bytes"
      cntr_type     = "in"
      cntr_class    = "lan" />
  </data_set>
  <data_set  name = "Network Xmit" >
    <cntr_attributes
      cntr_agg_flag = "TRUE"
      cntr_unit     = "bytes"
      cntr_type     = "out"
      cntr_class    = "lan" />
  </data_set>
  <data_set  name = "Network Transactions" >
    <cntr_attributes
      cntr_agg_flag = "TRUE"
      cntr_unit     = "trans"
      cntr_type     = "bidir"
      cntr_class    = "lan" />
  </data_set>


  <data_set  name = "Disk IOPS" >
    <cntr_attributes
      cntr_agg_flag = "TRUE"
      cntr_unit     = "calls"
      cntr_type     = "in"
      cntr_class    = "disk" />
    <cntr_attributes
      cntr_agg_flag = "TRUE"
      cntr_unit     = "calls"
      cntr_type     = "out"
      cntr_class    = "disk" />
  </data_set>
  <data_set  name = "Disk Read  IOPS" >
    <cntr_attributes
      cntr_agg_flag = "TRUE"
      cntr_unit     = "calls"
      cntr_type     = "in"
      cntr_class    = "disk" />
  </data_set>
  <data_set  name = "Disk Write IOPS" >
    <cntr_attributes
      cntr_agg_flag = "TRUE"
      cntr_unit     = "calls"
      cntr_type     = "out"
      cntr_class    = "disk" />
  </data_set>
  <data_set  name = "Disk Read  Throughput" >
    <cntr_attributes
      cntr_agg_flag = "TRUE"
      cntr_unit     = "bytes"
      cntr_type     = "in"
      cntr_class    = "disk" />
  </data_set>
  <data_set  name = "Disk Write Throughput" >
    <cntr_attributes
      cntr_agg_flag = "TRUE"
      cntr_unit     = "bytes"
      cntr_type     = "out"
      cntr_class    = "disk" />
  </data_set>

<!--  data sets used to calculate service demand -->
  <data_set
    name          = "Network Total Service Demand"
    sd_value_set  = "Network Total" />
  <data_set
    name          = "Network Transactions Service Demand"
    sd_value_set  = "Network Transactions" />
  <data_set
    name          = "DISK IOPS Service Demand"
    sd_value_set  = "DISK IOPS" />
      

<!--  report descriptions for each kind of test -->

  <report_each_test
    name            = "Default netperf4 network streams test instance report"
    version         = "1.0.0" >
  </report_each_test>
  <report_each_test
    name            = "Default netperf4 network rr test instance report"
    version         = "1.0.0" >
  </report_each_test>
  <report_each_test
    name            = "Default netperf4 disk test instance report"
    version         = "1.0.0" >
  </report_each_test>
  <report_each_test
    name            = "Default netperf4 dns test instance report"
    version         = "1.0.0" >
  </report_each_test>

<!--  report descriptions for summary and per run reports  -->
  <report_summary
    name            = "Default netperf4 Network streams tests Summary"
    version         = "1.0.0" >
    <report_field
      header1         = "INST"
      header2         = "NUM"
      header3         = " "
      units           = "none"
      width           = "-4"
      precision       = "0"
      type            = "count"
      when            = "run" >
    </report_field>
    <report_field
      header1         = "AVE"
      header2         = "Over"
      header3         = "Num"
      units           = "none"
      width           = "-4"
      precision       = "0"
      type            = "count"
      when            = "run" >
    </report_field>
    <report_field
      header1         = "SET"
      header2         = "Name"
      header3         = " "
      width           = "-6"
      type            = "tset"
      when            = "run&sum" >
    </report_field>
    <report_field
      data_set        = "Run Time"
      header1         = "TEST"
      header2         = "Time"
      header3         = "sec"
      width           = "-6"
      precision       = "2"
      type            = "value"
      when            = "run&sum" >
    </report_field>
    <report_field
      data_set        = "Run Time"
      header1         = "TEST"
      header2         = "Time"
      header3         = "sec"
      width           = "-6"
      precision       = "2"
      type            = "mean"
      when            = "sum" >
    <report_field
      data_set        = "Network Total"
      header1         = "DATA"
      header2         = "RATE"
      units           = "mbps"
      width           = "7"
      precision       = "2"
      type            = "value"
      when            = "run" >
    </report_field>
    <report_field
      data_set        = "Network Total"
      header1         = "AVE"
      header2         = "RATE"
      units           = "mbps"
      width           = "7"
      precision       = "2"
      type            = "mean"
      when            = "summary" >
    </report_field>
    <report_field
      data_set        = "Network Total"
      header1         = "conf"
      header2         = "+/-"
      units           = "mbps"
      width           = "7"
      precision       = "2"
      type            = "conf"
      when            = "summary" >
    </report_field>
    <report_field
      data_set        = "Network Recv"
      header1         = "RECV"
      header2         = "RATE"
      units           = "mbps"
      width           = "7"
      precision       = "2"
      type            = "value"
      when            = "run" >
    </report_field>
    <report_field
      data_set        = "Network Xmit"
      header1         = "XMIT"
      header2         = "RATE"
      units           = "mbps"
      width           = "7"
      precision       = "2"
      type            = "value"
      when            = "run" >
    </report_field>
    <report_field
      data_set        = "Network Total"
      header1         = "Min"
      header2         = "Rate"
      units           = "mbps"
      width           = "7"
      precision       = "2"
      type            = "min"
      when            = "summary" >
    </report_field>
    <report_field
      data_set        = "Network Total"
      header1         = "Max"
      header2         = "Rate"
      units           = "mbps"
      width           = "7"
      precision       = "2"
      type            = "max"
      when            = "summary" >
    </report_field>
    <report_field
      data_set        = "CPU Util"
      header1         = "CPU"
      header2         = "Util"
      units           = "%/100"
      width           = "6"
      precision       = "4"
      type            = "value"
      when            = "run" >
    </report_field>
    <report_field
      data_set        = "CPU Util"
      header1         = "CPU"
      header2         = "Util"
      units           = "%/100"
      width           = "6"
      precision       = "4"
      type            = "mean"
      when            = "summary" >
    </report_field>
    <report_field
      data_set        = "CPU Util"
      header1         = "+/-"
      header2         = "Util"
      units           = "%/100"
      width           = "6"
      precision       = "4"
      type            = "conf"
      when            = "summary" >
    </report_field>
    <report_field
      data_set        = "Network Total Service Demand"
      header1         = "SERV"
      header2         = "DEMAND"
      units           = "us/KB"
      width           = "6"
      precision       = "3"
      type            = "value"
      when            = "run" >
    </report_field>
    <report_field
      data_set        = "Network Total Service Demand"
      header1         = "SERV"
      header2         = "DEMAND"
      units           = "us/KB"
      width           = "6"
      precision       = "3"
      type            = "mean"
      when            = "summary" >
    </report_field>
    <report_field
      data_set        = "Network Total Service Demand"
      header1         = "SD"
      header2         = "+/-"
      units           = "us/KB"
      width           = "6"
      precision       = "3"
      type            = "conf"
      when            = "summary" >
    </report_field>
  </report_summary>

  <report_summary
    name            = "Default netperf4 Network rr tests Summary"
    version         = "1.0.0" >
  </report_summary>

  <report_summary
    name            = "Default netperf4 Disk tests Summary"
    version         = "1.0.0" >
  </report_summary>

</report_description>
